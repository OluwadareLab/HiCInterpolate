{
    "name": "HiCInterpolate Debug",
    "type": "python",
    "request": "launch",
    "module": "torch.distributed.run",
    "args": [
        "--nproc-per-node=1",
        "--nnodes=1",
        "--node_rank=0",
        "train.py",
        "--epochs",
        "100",
        "--batch-size",
        "32",
        "--save-every",
        "10",
        "--distributed"
    ],
    "console": "integratedTerminal",
    "justMyCode": false,
    "env": {
        "PYTHONUNBUFFERED": "1"
    }
}